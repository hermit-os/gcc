{
     "package": {
         "name": "gcc-hermit-bootstrap",
         "repo": "ubuntu",
         "subject": "hermitcore",
         "desc": "Cross-toolchain for HermitCore",
         "website_url": "https://hermitcore.org",
         "issue_tracker_url": "https://github.com/RWTH-OS/HermitCore/issues",
         "vcs_url": "https://github.com/hermitcore/gcc.git",
         "github_use_tag_release_notes": false,
         "github_release_notes_file": "RELEASE",
         "licenses": ["GPL2"],
         "public_download_numbers": false,
         "public_stats": false
     },

     "version": {
         "name": "6.3.0",
         "desc": "Bootstrap compiler for HermitCore",
	 "gpgSign": false
     },

     "files":
     [{
       "includePattern": "../(gcc[^/]*deb$)", "uploadPattern": "$1",
       "matrixParams": {
         "deb_distribution": "vivid",
         "deb_component": "main",
         "deb_architecture": "amd64",
         "override": 1}
     }],
     "publish": true
 }
